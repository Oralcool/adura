{
  "expo": {
    "name": "adura",
    "slug": "adura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "adura",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1621"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0E1621"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jossyoro01.adura"
    },
    "plugins": [
      "expo-router",
      "expo-audio"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b9687ff6-2663-40c1-8b0e-f6f2ad9a7a82"
      }
    }
  }
}
